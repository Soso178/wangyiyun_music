.banners{
  width: 100%;
  height: 300rpx;
}
.banners image{
  width: 100%;
  height: 100%;
}
.navContainer{
  width: 100%;
  display: flex;
}
.navItem{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 20%;
}
.navItem  .iconfont{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 100rpx;
  background-color: red;
  color: white;
  margin: 20rpx 0;
  font-size: 50rpx;
}
.navItem text{
  font-size: 26rpx;
}
.recommendContainer{
  padding: 20rpx;

}
.recommendScroll{
  padding-top: 10rpx;
  display: flex;
  height: 300rpx;
}
.scrollItem{
  width: 200rpx;
  margin-right: 20rpx;
  font-size: 26rpx;
}
.scrollItem image{
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
}
.scrollItem text{
  font-size: 26rpx;
  overflow: hidden;
  display: -webkit-box;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.topList{
  padding: 0 20rpx;
}
.swiperItem{
  background-color: #fbfbfb;
  width: 96%;
}
.topListSwiper{
  height: 450rpx;
}
.swiperItem .title{
  font-size: 30rpx;
  line-height: 80rpx;
}
.musicItem{
  display: flex;
  margin-bottom: 20rpx;
}
.musicItem image{
  width: 100rpx;
  height: 100rpx ;
}
.musicItem .count{
  width: 100rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
}
.musicItem .musicName{
  height: 100rpx;
  line-height: 100rpx;
  max-width: 400rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}